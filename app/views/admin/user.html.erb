<div class="data-table">
  <p class="tableTitle">ユーザーのデータ一覧</p>
  <table class="table">
    <thead>
      <tr>
        <th>id名</th>
        <th>メールアドレス</th>
      </tr>
    </thead>
    <tbody>
      <% @user.each do |user| %>
        <tr>
          <td>
            <%= user.id %>
          </td>
          <td>
            <%= user.email %>
          </td>
        </tr>
        <% end %>
    </tbody>
  </table>
</div>